<link href="~/Content/font-awesome.min.css" rel="stylesheet" />
@model IEnumerable<DataViewModels.ItemPartyTypeView>

@{
    ViewBag.Title = "GetAll";
}

<h2>Type of people related to Library</h2>

@*<p>
    @Html.ActionLink("Create New", "Add")
</p>*@
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PartyTypeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PartyTypeDescription)
        </th>

        <th>
            
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PartyTypeName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PartyTypeDescription)
            </td>
            <td>
                @*<a class="btn btn-danger btn-sm" href="/ItemPartyType/Delete?id=@item.ItemPartyTypeID" onclick="return confirm( 'Are you sure to delete this record ?')"><i class="fa fa-trash fa-lg"></i></a>*@
                @*<a href="/ItemPartyType/Edit?id=@item.ItemPartyTypeID" class="btn btn-primary btn-sm" name="Name"><i class="fa fa-pencil fa-lg"></i></a>*@
                <a href="/ItemPartyType/Details?id=@item.ItemPartyTypeID" class="btn btn-primary btn-sm" name="Name"><i class="fa fa-info-circle fa-lg"></i></a>
            </td>
        </tr>
    }

</table>
